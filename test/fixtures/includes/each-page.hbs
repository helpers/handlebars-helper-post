{{#each pages}}
  {{#isnt this.basename "index"}}
    {{#process}}
      <!-- page -->
      <section class="component">
        <h4>{{{data.title}}} <a href="{{relative ../../../page.dest this.dest}}"> <small class="pull-right">visit component &rarr;</small></a></h4>
        <div class="bs-example">
          {{{page}}}
        </div>
      </section>
    {{/process}}
  {{/isnt}}
{{/each}}